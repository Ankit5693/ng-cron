{
  "version": 1,
  "projects": {
    "bootstrap-example": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/bootstrap-example",
      "sourceRoot": "apps/bootstrap-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/bootstrap-example",
            "index": "apps/bootstrap-example/src/index.html",
            "main": "apps/bootstrap-example/src/main.ts",
            "polyfills": "apps/bootstrap-example/src/polyfills.ts",
            "tsConfig": "apps/bootstrap-example/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/bootstrap-example/src/favicon.ico",
              "apps/bootstrap-example/src/assets"
            ],
            "styles": ["apps/bootstrap-example/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bootstrap-example/src/environments/environment.ts",
                  "with": "apps/bootstrap-example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bootstrap-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bootstrap-example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bootstrap-example:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/bootstrap-example/tsconfig.app.json",
              "apps/bootstrap-example/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/bootstrap-example/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/bootstrap-example/jest.config.js",
            "tsConfig": "apps/bootstrap-example/tsconfig.spec.json",
            "setupFile": "apps/bootstrap-example/src/test-setup.ts"
          }
        }
      }
    },
    "cron-bootstrap": {
      "projectType": "library",
      "root": "libs/cron-bootstrap",
      "sourceRoot": "libs/cron-bootstrap/src",
      "prefix": "cron",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/cron-bootstrap/tsconfig.lib.json",
            "project": "libs/cron-bootstrap/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/cron-bootstrap/tsconfig.lib.json",
              "libs/cron-bootstrap/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/cron-bootstrap/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/cron-bootstrap/jest.config.js",
            "tsConfig": "libs/cron-bootstrap/tsconfig.spec.json",
            "setupFile": "libs/cron-bootstrap/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "build": {
			"builder": "@angular-devkit/build-ng-packagr:build",
			"options": {
				"tsConfig": "libs/core/tsconfig.lib.json",
				"project": "libs/core/ng-package.json"
			}
		},
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core/tsconfig.lib.json",
              "libs/core/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "tsConfig": "libs/core/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "bootstrap-example"
}
